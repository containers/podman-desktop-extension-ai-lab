{
  "name": "ai-lab",
  "displayName": "AI Lab",
  "description": "Podman Desktop AI Lab",
  "version": "0.4.0-next",
  "icon": "icon.png",
  "publisher": "redhat",
  "license": "Apache-2.0",
  "engines": {
    "podman-desktop": "^1.6.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "brain-icon": {
        "description": "Brain icon",
        "default": {
          "fontPath": "brain.woff2",
          "fontCharacter": "\\E001"
        }
      }
    },
    "views": {
      "icons/containersList": [
        {
          "when": "ai-lab-model-id in containerLabelKeys",
          "icon": "${brain-icon}"
        }
      ],
      "icons/image": [
        {
          "when": "ai-lab-recipe-id in imageLabelKeys",
          "icon": "${brain-icon}"
        }
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "watch": "vite --mode development build -w"
  },
  "dependencies": {
    "fast-xml-parser": "^4.3.6",
    "mustache": "^4.2.0",
    "openai": "^4.29.2",
    "postman-code-generators": "^1.9.0",
    "postman-collection": "^4.4.0",
    "simple-git": "^3.24.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@podman-desktop/api": "0.0.202403281426-8a6379d",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/postman-collection": "^3.5.10",
    "vitest": "^1.4.0"
  }
}
