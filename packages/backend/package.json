{
  "name": "ai-studio",
  "displayName": "AI Studio",
  "description": "Podman Desktop AI Studio",
  "version": "0.1.0-next",
  "icon": "icon.png",
  "publisher": "redhat",
  "license": "Apache-2.0",
  "engines": {
    "podman-desktop": "^1.6.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "brain-icon": {
        "description": "Brain icon",
        "default": {
          "fontPath": "brain.woff2",
          "fontCharacter": "\\E001"
        }
      }
    },
    "views": {
      "icons/containersList": [
        {
          "when": "ai-studio-model-id in containerLabelKeys",
          "icon": "${brain-icon}"
        }
      ],
      "icons/image": [
        {
          "when": "ai-studio-recipe-id in imageLabelKeys",
          "icon": "${brain-icon}"
        }
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "watch": "vite --mode development build -w"
  },
  "dependencies": {
    "openai": "^4.28.0",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@podman-desktop/api": "^0.0.202402080712-0f5d4ce",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18",
    "vitest": "^1.3.1"
  }
}
